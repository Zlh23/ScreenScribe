{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions plus save dialog and file write",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:allow-save",
    {
      "identifier": "fs:allow-write",
      "allow": [
        { "path": "$HOME/**" },
        { "path": "$DOWNLOAD/**" }
      ]
    }
  ]
}
